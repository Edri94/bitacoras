    <section name="escribeArchivoLOG"
      type="Pruebas_Bitacoras.Configuracion.EscribeArchivoLOG.EscribeArchivoLOGConfig, Pruebas_Bitacoras"/>
    </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <escribeArchivoLOG>
      <instances>
        <add key="logFileName"
          value="LogBitacoras.log"/>
      </instances>
    </escribeArchivoLOG>
</configuration>