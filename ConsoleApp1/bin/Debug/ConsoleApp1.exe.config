<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
      <section name="escribeArchivoLOG"
        type="BitacorasNET.Configuracion.EscribeArchivoLOG.EscribeArchivoLOGConfig, BitacorasNET"/>
      </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
  <appSettings>
    <add key="mensaje" value="hola mundo desde app.config ConsoleApp1"/>
  </appSettings>
  <escribeArchivoLOG>
    <instances>
      <add key="logFileName"
        value="LogBitacoras.log"/>
    </instances>
  </escribeArchivoLOG>
</configuration>